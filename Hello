local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("itzm hub", "Synapse")

-- Tabs
local Home = Window:NewTab("Home")
local HomeSection = Home:NewSection("Home")

local Player = game.Players.LocalPlayer

local playerIdLabel = HomeSection:NewLabel("Player ID: " .. Player.UserId)
local playerNameLabel = HomeSection:NewLabel("Player Name: " .. Player.Name)
local displayNameLabel = HomeSection:NewLabel("Display Name: " .. Player.DisplayName)
local fpsLabel = HomeSection:NewLabel("FPS: Calculating...")

local function updatePlayerInfo()
    -- Update player information
    playerIdLabel:UpdateText("Player ID: " .. Player.UserId)
    playerNameLabel:UpdateText("Player Name: " .. Player.Name)
    displayNameLabel:UpdateText("Display Name: " .. Player.DisplayName)

    -- Update FPS
    local success, fpsInfo = pcall(function()
        return game:GetService("Stats"):GetFPS()
    end)

    if success and fpsInfo ~= nil then
        local fps = math.floor(fpsInfo)
        local targetFps = math.floor(fpsInfo * 100 + 0.5) / 100
        fpsLabel:UpdateText("FPS: " .. fps .. " (Target: " .. targetFps .. ")")
    else
        fpsLabel:UpdateText("FPS: Error")
    end

    -- Update player count
    local playerCountLabel = HomeSection:FindLabel("Player Count:")
    if playerCountLabel then
        local success, playerCount = pcall(function()
            return #game.Players:GetPlayers()
        end)

        if success and playerCount ~= nil then
            playerCountLabel:UpdateText("Player Count: " .. playerCount)
        else
            playerCountLabel:UpdateText("Player Count: Error")
        end
    end
end

game:GetService("RunService").Heartbeat:Connect(updatePlayerInfo)

HomeSection:NewButton("Rejoin", "Rejoins the game", function()
    local function rejoinPlayer(player)
        local success, errorMessage = pcall(function()
            game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId, player)
        end)

        if not success then
            warn("Failed to rejoin player:", errorMessage)
        end
    end

    for _, player in ipairs(game.Players:GetPlayers()) do
        if player.UserId > 0 then
            rejoinPlayer(player)
        end
    end
end)

HomeSection:NewButton("Clear Labels", "Resets the labels", function()
    playerIdLabel:UpdateText("Player ID: ")
    playerNameLabel:UpdateText("Player Name: ")
    displayNameLabel:UpdateText("Display Name: ")
    fpsLabel:UpdateText("FPS: ")
    HomeSection:RemoveLabel("Player Count:")
end)

print("Code has been executed successfully!")

