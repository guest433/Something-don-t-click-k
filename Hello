Script3Section:NewSlider("walkspeed", "", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
Script3Section:NewSlider("JumpPower", "", 500, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

Script3Section:NewToggle("advance speedwalking", "", function(state)
    if state then
        _G.Running = true
        RunMovement()
    else
        _G.Running = false
        StopMovement()
    end
end)

local Player = game:GetService("Players").LocalPlayer

function RunMovement()
    while _G.Running do
        local humanoidRootPart = Player.Character and Player.Character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart then
            local forwardDirection = humanoidRootPart.CFrame.lookVector
            Player.Character.Humanoid:MoveTo(Player.Character.HumanoidRootPart.Position + forwardDirection * 1)
            local forwardDirection = humanoidRootPart.CFrame.lookVector
            Player.Character.Humanoid:MoveTo(Player.Character.HumanoidRootPart.Position + forwardDirection * 1)
        end
        wait()
    end
end

function StopMovement()
    Player.Character.Humanoid:MoveTo(Player.Character.HumanoidRootPart.Position)
end

